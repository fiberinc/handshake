{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  // "disableSources": true,
  "excludeExternals": true,
  // "excludeGroups": true,
  "excludeInternal": true,
  // "excludeNotDocumented": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "gitRevision": "main",
  // "hideBreadcrumbs": true,
  // "hideHierarchy": true,
  // "hideKindPrefix": true,
  "hideGenerator": true,
  "kindSortOrder": [
    "Function",
    "TypeAlias",
    "Interface",
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Class",
    "Constructor",
    "Property",
    "Variable",
    "Accessor",
    "Method",
    // "ObjectLiteral",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  // "outputFileStrategy": "modules",
  "readme": "none",
  "skipErrorChecking": true,
  "sort": [
    "kind",
    "static-first",
    "required-first",
    "alphabetical"
  ]
}